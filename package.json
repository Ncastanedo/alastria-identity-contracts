  "license": "MIT",
  "dependencies": {
    "@truffle/config": "^1.2.33",
    "@truffle/hdwallet-provider": "^1.1.1",
    "keythereum": "^1.2.0",
    "remixd": "^0.2.4-alpha.0",
    "truffle": "^5.1.53"
  }
}
